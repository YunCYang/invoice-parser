{
  "name": "invoice-parser",
  "version": "1.0.0",
  "description": "Simple client side invoice parser.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "sass": "sass public/styles.sass public/styles.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YunCYang/invoice-parser.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YunCYang/invoice-parser/issues"
  },
  "homepage": "https://github.com/YunCYang/invoice-parser#readme",
  "eslintIgnore": [
    "server/public/main.js"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "plugin:react/recommended",
      "eslint:recommended",
      "standard"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "padded-blocks": 0,
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "curly": [
        "error",
        "multi-line"
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "always",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "react/prop-types": 0
    }
  },
  "dependencies": {
    "dotenv": "8.2.0",
    "papaparse": "5.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "sass": "1.26.11",
    "session-file-store": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "eslint": "7.10.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.2",
    "eslint-plugin-standard": "4.0.1",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12"
  }
}
